DELIMITER // 
CREATE PROCEDURE updateBillStatus (IN updatedStatus CHAR(50)) 
BEGIN 
UPDATE bills SET updatedStatus = status 
WHERE code = bill_code;
END // 
DELIMITER ; 

DELIMITER // 
CREATE PROCEDURE insertBill (IN newStatus CHAR(50)) 
BEGIN 
INSERT INTO bills (oldStatus,newStatus) VALUES (stuff , status) ON DUPLICATE update status=closed 
END // 
DELIMITER ; 

DELIMITER // 
CREATE PROCEDURE deleteBillByCode (IN deleteCode CHAR(40)) 
BEGIN 
DELETE FROM bills
WHERE code = deleteCode;
END // 
